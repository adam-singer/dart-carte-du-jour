<html>
  <head>
    <title>Build History</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css">
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-51687523-1', 'dartdocs.org');
      ga('send', 'pageview');

    </script>
  </head>
  <body>
    <!-- TODO(adam): remove after end of tech preview -->
    <p class="bg-danger text-center" style="padding: 0px; margin: 0px 0px 0px;">
      <em><strong>Technology preview</strong></em>
    </p>  
    <nav class="navbar navbar-default" role="navigation">
      <div class="container-fluid">
        <ul class="nav navbar-nav">
          <li><a href="/">Home</a></li>
          <li class="active"><a href="/history/index.html">Build history</a></li>
          <li><a href="/failed/index.html">Build failures</a></li>
        </ul>
      </div>
    </nav>

    <div class='container'>
      <table class="table table-hover">
        <thead>
          <tr>
            <th>Package</th>
            <th>Time</th>
            <th>Status</th>
            <th>Log</th>
          </tr>
        </thead>
        <tbody>
          {{#packages}}
            <tr class='{{^isBuilt}}danger{{/isBuilt}}'>
              <td>
                {{#isBuilt}}
                  <a href="/documentation/{{name}}/{{version}}/index.html#{{name}}">{{name}}-{{version}}</a>
                {{/isBuilt}}
                {{^isBuilt}}
                  {{name}}-{{version}}
                {{/isBuilt}}
              </td>
              <td>{{timestamp}}</td>
              <td>
                {{#isBuilt}}
                  Success
                {{/isBuilt}}
                {{^isBuilt}}
                  <strong>FAILURE</strong>
                {{/isBuilt}}
              </td>
              <td><a href="{{buildLogUrl}}">build log</a></td>
            </tr>
          {{/packages}}
        </tbody>
      </table>
    </div>
  </body>
</html>
